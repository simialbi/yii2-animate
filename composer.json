{
	"name": "simialbi/yii2-animate",
	"description": "animate.css from daneden for usage in yii framework",
	"keywords": [
		"yii2",
		"animate.css",
		"animate",
		"daneden"
	],
	"type": "yii2-extension",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/simialbi/yii2-animate/issues",
		"source": "https://github.com/simialbi/yii2-animate"
	},
	"authors": [
		{
			"name": "Simon Karlen",
			"email": "simi.albi@outlook.com"
		}
	],
	"minimum-stability": "stable",
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"require": {
		"yiisoft/yii2": "~2.0",
		"simialbi/yii2-simialbi-base": "*",
		"bower-asset/animate.css": "~3.7"
	},
	"config": {
		"process-timeout": 1800,
		"fxp-asset": {
			"enable": false
		}
	},
	"autoload": {
		"psr-4": {
			"simialbi\\yii2\\animatecss\\": ""
		}
	}
}